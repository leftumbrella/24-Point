#include <iostream>
//#include <stdlib.h>
using namespace std;
int clac(int a,int b,char c);
int main(){
	while(1){
	//fflush(NULL); 
	int num[4],tem;
	for(int i=0;i<4;i++){
		cin>>num[i];
	}
	char ch[4]={'+','-','*','/'};
	for(int i=0;i<4;i++){
		for(int j=0;j<4;j++){
			if(i!=j){
				for(int k=0;k<4;k++){
					tem=0; 
					tem=clac(num[i],num[j],ch[k]);
					if(tem!=-1){
						for(int l=0;l<4;l++){
							if(l!=j&&l!=i){
								for(int m=0;m<4;m++){
									tem=clac(num[i],num[j],ch[k]);
									tem=clac(tem,num[l],ch[m]);
									if(tem!=-1){
										for(int n=0;n<4;n++){
											if(n!=l&&n!=i&&n!=j){
												for(int p=0;p<4;p++){
													if(clac(tem,num[n],ch[p])==24){
														cout<<num[i]<<ch[k]<<num[j]<<ch[m]<<num[l]<<ch[p]<<num[n]<<endl;
														goto a;
													}
												} 
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
	a:;
	}
	return 0;
} 
int clac(int a,int b,char c){
	double tem=0.0;
	int ret=0;
	if(c=='+'){
		ret=a+b;
	}else if(c=='-'){
		//if(a-b<0){
		//	ret=-1;
		//}else{
			ret=a-b;
		//}
	}else if(c=='*'){
		ret=a*b;
	}else if(c=='/'){
		tem=a*1.0/b;
		if(tem!=((a/b)*1.0)){
			ret=-1;
		}else{
			ret=a/b;
		}
	}
	return ret;
}
